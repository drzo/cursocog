# set term png small color

set xrange [0:250]

#################################################
#
# set term png
# set out 'r11-cls-nwords.png'
set term postscript eps enhanced color 20
set out 'r11-cls-nwords.eps'
set key right top
set title "Number of Words in Class (run-11; q=0.7)"
set xlabel "Merge ordinal"
set ylabel "N"
unset yrange

plot "r11-p-cls-q0.7-c0.2-n4.dat" using 1:3 with lines lw 2 title "n=4 precise", \
     "r11-cls-q0.7-c0.2-n4.dat" using 1:3 with lines lw 2 title "n=4 imprecise", \
     "r11-cls-q0.7-c0.2-n3.dat" using 1:3 with lines lw 2 title "n=3 imprecise", \
     "r11-cls-q0.7-c0.2-n2.dat" using 1:3 with lines lw 2 title "n=2 imprecise", \
     "r11-cls-q0.7-c0.2-n1.dat" using 1:3 with lines lw 2 title "n=1 imprecise"

